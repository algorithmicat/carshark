    "components": {
        "schemas": {
            "CarEventStoreRequest": {
                "required": [
                    "date",
                    "latitude",
                    "langitude",
                    "fuel",
                    "speed",
                    "renter_id",
                    "car_id",
                    "car_status_id"
                ],
                "properties": {
                    "date": {
                        "description": "Дата и время события",
                        "type": "dateTime",
                        "example": "2016-11-08 07:05:41"
                    },
                    "latitude": {
                        "description": "Широта",
                        "type": "number",
                        "format": "float",
                        "example": "35.55242"
                    },
                    "langitude": {
                        "description": "Долгота",
                        "type": "number",
                        "format": "float",
                        "example": "23.343332"
                    },
                    "fuel": {
                        "description": "Топливо",
                        "type": "integer",
                        "example": "23"
                    },
                    "speed": {
                        "description": "Скорость",
                        "type": "integer",
                        "example": "44"
                    },
                    "renter_id": {
                        "description": "id арендатора",
                        "type": "integer",
                        "example": "3"
                    },
                    "car_id": {
                        "description": "id машины",
                        "type": "integer",
                        "example": "3"
                    },
                    "car_status_id": {
                        "description": "id статуса машины",
                        "type": "integer",
                        "example": "3"
                    }
                },
                "type": "object"
            },
            "CarMarkStoreRequest": {
                "required": [
                    "name"
                ],
                "properties": {
                    "name": {
                        "description": "Модель машины",
                        "type": "string",
                        "example": "kia"
                    }
                },
                "type": "object"
            },
            "CarModelStoreRequest": {
                "required": [
                    "name",
                    "car_mark_id"
                ],
                "properties": {
                    "name": {
                        "description": "Модель машины",
                        "type": "string",
                        "example": "rio"
                    },
                    "car_mark_id": {
                        "description": "Марка машины",
                        "type": "integer",
                        "example": "3"
                    }
                },
                "type": "object"
            },
            "CarStatusStoreRequest": {
                "required": [
                    "car_status"
                ],
                "properties": {
                    "car_status": {
                        "description": "Статус машины",
                        "type": "integer",
                        "example": "2"
                    }
                },
                "type": "object"
            },
            "CarStoreRequest": {
                "required": [
                    "number",
                    "mileage",
                    "car_statuses_id",
                    "year_of_release",
                    "car_model_id"
                ],
                "properties": {
                    "number": {
                        "description": "Номер машины",
                        "type": "string",
                        "example": "ц135вы"
                    },
                    "mileage": {
                        "description": "Пробег машины",
                        "type": "integer",
                        "example": "352"
                    },
                    "car_statuses_id": {
                        "description": "id статуса машины",
                        "type": "integer",
                        "example": "2"
                    },
                    "year_of_release": {
                        "description": "Год выпуска машины",
                        "type": "integer",
                        "example": "2003"
                    },
                    "car_model_id": {
                        "description": "id модели",
                        "type": "integer",
                        "example": "3"
                    }
                },
                "type": "object"
            },
            "OperationStoreRequest": {
                "required": [
                    "date",
                    "sum",
